generator client {
    provider = "prisma-client-js"
    output   = "../../../node_modules/@prisma-clients/jobber-auth"
}

datasource db {
    provider = "postgresql"
    url      = env("AUTH_DATABASE_URL")
}

model User {
    id       Int    @id @default(autoincrement())
    email    String @unique
    password String
    // isVerified Boolean?
    // role       Role     @default(USER)
}

